<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nguegangbeth.openprices">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logos"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.UserListActivity">

        </activity>
        <activity android:name=".views.ComparerProduitActivity" />
        <activity android:name=".views.Choix_authentificationActivity" />
        <activity android:name=".views.DefautListProductActivity" />
        <activity
            android:name=".views.BottomNavigatorActivity"
            android:windowSoftInputMode="stateHidden"></activity>
        <activity android:name=".views.Interface_connexionActivity" />
        <activity android:name=".views.StartActivity" />
        <activity android:name=".views.User_no_connectActivity" />
        <activity android:name=".views.InscriptionActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.DescriptionActivity" />
    </application>

</manifest>